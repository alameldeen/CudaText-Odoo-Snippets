name=Odoo Asset Template BackEnd
id=otbackend
lex=XML
text=
<template id="assets_backend" name="${1:module_name} backend assets" inherit_id="web.assets_backend">
    <xpath expr="." position="inside">
        <link rel="stylesheet" href="/${1:module_name}/static/src/css/${1:module_name}.css"/>
        <script type="text/javascript" src="/${1:module_name}/static/src/js/${1:module_name}.js"></script>
    </xpath>
</template>
